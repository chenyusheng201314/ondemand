.person-ul, .person-ul li, .person-ul .person-from {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.person-ul {
  margin-top: -50px;
}

.person-ul li {
  border-bottom: 1px solid #CCCCCC;
  padding: 60px 0;
}

.up-head {
  float: left;
  width: 200px;
  height: auto;
  overflow: hidden;
  text-align: center;
  margin-top: 50px;
  position: relative;
}

.up-head img {
  width: 112px;
  height: 112px;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
}

.up-head p {
  width: 112px;
  text-align: center;
  font-size: 16px;
  color: #666;
  margin: 10px auto;
}

.up-head .up-pic {
  width: 112px;
  height: 112px;
  position: absolute;
  left: 45px;
  z-index: 10;
  top: 0;
  opacity: 0;
}

.up-head .up-pic .el-upload {
  width: 112px;
  height: 112px;
}

.person-from ul {
  float: left;
  width: 420px;
  margin-left: 100px;
  height: auto;
  overflow: hidden;
}

.person-from ul li {
  width: 100%;
  height: auto;
  overflow: hidden;
}










































