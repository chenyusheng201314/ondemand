* {
  margin: 0;
  padding: 0;
  list-style: none;
}

body {
  font-size: 14px;
  font-family: MicrosoftYaHei;
  -webkit-font-smoothing: antialiased;
}

table {
  border-collapse: collapse;
  border-spacing: 0;
}

a {
  color: #000;
  text-decoration: none;
}

ol, ul, li {
  list-style-type: none;
}

input {
  outline: none;
}

img {
  vertical-align: middle;
  border: 0;
}
.con{
  width: 100%;
  height: auto;
  min-width: 1200px;
  overflow-x: auto;
}















